{
  "name": "llama-node",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "llama-node",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.14.8",
    "@types/node": "^18.15.3",
    "ava": "^5.1.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",

    "test": "ava",
    "start": "node __test__/index.spec.mjs",
    "universal": "napi universal",
    "version": "napi version"
  },
  "repository": {
    "url": "https://github.com/hlhr202/llama-node"
  },
  "homepage": "https://github.com/hlhr202/llama-node",
  "packageManager": "yarn@3.5.0",
  "optionalDependencies": {
    "llama-node-win32-x64-msvc": "0.0.0",
    "llama-node-darwin-x64": "0.0.0",
    "llama-node-linux-x64-gnu": "0.0.0",
    "llama-node-darwin-arm64": "0.0.0",
    "llama-node-android-arm64": "0.0.0",
    "llama-node-linux-arm64-gnu": "0.0.0"
  }
}